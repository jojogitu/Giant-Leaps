{{!< default}}
<div class="col-lg-12">
  {{> "headlines"}}
</div>

{{!-- Blog Entries Column --}}
<div class="col-md-8">
  <div class="page-header">
    <h4 class="has-sub">Latest Posts</h4>
  </div>
  {{> "loop"}}

</div>
{{!-- ./Blog Entries Column --}}

{{!-- Blog Sidebar Widgets Column --}}
<div class="col-md-4">
  {{> "sidebar"}}
</div>
{{!-- ./Blog Sidebar Widgets Column --}}

</div>

<!-- The overlay -->
<div id="overl" class="overlay">
  <!-- Button to close the overlay navigation -->
  <a href="javascript:void(0)" class="closebtn" onclick="closeOverlay()">&times;</a>
  <!-- Overlay content -->
  <div class="overlay-contentx" id="canvas">
    <div class="zoom-icon zoom-icon-in"></div>

    <div class="magazine-viewport">
      <div class="container">
        <div class="magazine">
          <!-- Next button -->
          <div ignore="1" class="next-button"></div>
          <!-- Previous button -->
          <div ignore="1" class="previous-button"></div>
        </div>
      </div>
    </div>
    
  </div>
</div>
